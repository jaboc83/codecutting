{
  "name": "codecutting",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react": "16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.2",
    "rimraf": "^3.0.0",
    "typescript": "3.6.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.24",
    "@fortawesome/free-regular-svg-icons": "^5.11.1",
    "@fortawesome/free-solid-svg-icons": "^5.11.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "ajv": "^6.10.2",
    "autoprefixer": "^9.6.1",
    "cross-env": "^5.2.1",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^5.0.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "postcss-cli": "^6.1.3",
    "prettier": "^1.18.2",
    "tailwindcss": "^1.1.2"
  },
  "scripts": {
    "css:watch": "postcss src/assets/css/tailwind.css -o src/assets/css/compiled.css -w",
    "css:build": "postcss src/assets/css/tailwind.css -o src/assets/css/compiled.css",
    "react:start": "rimraf ./build && react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "cross-env CI=true react-scripts test --env=jsdom",
    "react:eject": "react-scripts eject",
    "eslint": "eslint ./src/",
    "start": "run-p react:start css:watch",
    "build": "run-s css:build react:build",
    "test": "run-s react-test",
    "eject": "run-s react:eject",
    "lint": "run-s eslint"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
